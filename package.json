{
  "name": "called-with-immutable",
  "version": "0.1.4",
  "description": "Custom Jest matchers for working with Immutable.js types in spies",
  "main": "dist/bundle.js",
  "author": "Philipp Spie√ü",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "format": "prettier --write \"src/**/*.js\"",
    "build": "rollup -c",
    "release": "yarn test && flow && yarn build && yarn publish"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/pretty-format/build/plugins/ConvertAnsi.js"
    ]
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "flow-bin": "^0.46.0",
    "immutable": "^3.8.1",
    "jest": "^20.0.3",
    "prettier": "^1.3.1",
    "pretty-format": "^20.0.3",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "jest-matcher-utils": "^20.0.3",
    "jest-matchers": "^20.0.3"
  }
}
